<tr>
    {{#displaySelector}}
      <td class="line-check-box">
        <input type="checkbox" class="patch-line-checker {{type}}-checkbox" name="patchline[]" value="{{selectorNumber}}" checked />
      </td>
    {{/displaySelector}}

    <td class="{{lineClass}} {{type}}">
      {{{lineNumber}}}
    </td>
    <td class="{{type}}">
        <div class="{{contentClass}} {{type}}">
        {{#prefix}}
            <span class="d2h-code-line-prefix">{{{prefix}}}</span>
        {{/prefix}}
        {{#content}}
            <span class="d2h-code-line-ctn">{{{content}}}</span>
        {{/content}}
        </div>
    </td>
</tr>
